<apex:page showHeader="false" sidebar="false" id="needsAssesmentIframe" standardController="dealer__Sales_Up__c" extensions="NeedsAnalysisAccount">
	<apex:form >
			<apex:pageBlock mode="maindetail">
			<apex:pageBlockButtons >
				<apex:commandButton value="Save" action="{!saveNeedsAssesment}" />
			</apex:pageBlockButtons>
            <apex:pageBlockSection columns="2">
            <apex:inputField value="{!NeedsAnalysis.dtffa__End_User__c}" />
            <apex:inputField value="{!NeedsAnalysis.dtffa__A__c}" />

            <apex:inputField value="{!NeedsAnalysis.dtffa__Physical_Challenge__c}" />
            <apex:inputField value="{!NeedsAnalysis.dtffa__B__c}" />

            <apex:inputField value="{!NeedsAnalysis.Claim_Number__c}" />
            <apex:inputField value="{!NeedsAnalysis.dtffa__C__c}" />

            <apex:inputField value="{!NeedsAnalysis.dtffa__Actual_Estimate__c}" />
            <apex:inputField value="{!NeedsAnalysis.dtffa__Chair_Dimensions_Diagram__c}" />

            <apex:inputField value="{!NeedsAnalysis.dtffa__Chair_Brand__c}" />
            <apex:outputText />

            <apex:inputField value="{!NeedsAnalysis.dtffa__Chair_Model__c}" />
            <apex:outputText />

            <apex:inputField value="{!NeedsAnalysis.dtffa__Client_In_Chair_Weight__c}" />
            <apex:inputField value="{!NeedsAnalysis.dtffa__PlannedChanges__c}" />
            
            </apex:pageBlockSection>
            </apex:pageBlock>
	</apex:form>
</apex:page>