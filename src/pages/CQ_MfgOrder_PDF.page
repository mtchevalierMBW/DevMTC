<apex:page renderAs="pdf" sidebar="false" applyHtmlTag="false" applyBodyTag="false" showHeader="false" showChat="false" standardStylesheets="false" 
		   standardController="CommercialQuote__c" extensions="CQ_EXT">
<html>
<head>
    <!--
    	2017-03-03	B. Leaman	Copy from CQ_PDF2 & adjust to be a manufacturer order (with cost instead of price)
    -->
    <apex:stylesheet value="//fonts.googleapis.com/css?family=Open+Sans" />
  <style>

    body {
      font-family: 'PT Sans', Dialog;
      font-size: 11px;
    }

    strong {
      font-weight: 700;
    }

    table {
      border-right:  1px solid #000;
      border-bottom:  1px solid #000;
    }

    td, th {
      border-top: 1px solid #000;
      border-left: 1px solid #000;
      padding: 2px 5px;
      border-bottom: 0;
      border-right: 0;
      color: #111;
    }

    .pull_right {
      float: right;
    }

    .center_text {
      text-align: center;
    }

    .right_text {
      text-align: right;
    }

    .disclaimer {
      font-size: .8em;
      color: #333;
    }

    .form_data {
      font-family: 'Monaco', 'Courier New', Courier, monospace;
      /*float: right;*/
      /*font-size: 1.1em;*/
      font-weight: bold;
      margin-left: 5px;
      color: #000;
    }

    .title_cell {
      background-color: #444;
      text-align: center;
      color: #fff;
      font-weight: bold;
    }

    .header_cell, th {
      background-color: #DDD;
      text-align: center;
    }

    .quote_data td {
      vertical-align: top;
      height: 30px;
    }

    .label {
      font-size: 7px;
      color: #333;
      clear: both;
    }

    .spacers td {
      border-right: 0px;
      border: 0px solid #000;
    }

    .spacers tr {
      border: 0px solid #fff;
    }

    p {
      margin: 0;
      padding: 0;
    }

    @page {
    /* Landscape orientation */
    size:portrait;
    margin: 0.15in;

        /* Put page numbers in the top right corner of each
        page in the pdf document. */
        @bottom-left {
            font-family: 'PT Sans', Dialog;
            width: 600px;
            font-size: 12px;
            content: "Commercial {!doctypename}";	/* BLL6c was Quote */
        }
        @bottom-right {
          font-family: 'PT Sans', Dialog;
          width: 200px;
          font-size: 10px;
          content: "Page " counter(page) " of " counter(pages);
        }
    }
    td.qty { width: 50px;}
    tbody.head{page-break-before:avoid;page-break-after:avoid;}
    tbody.options {page-break-before:avoid;page-break-inside:auto;}
    tr {page-break-inside:avoid;}
  </style>
</head>

